<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Bazytrack POI Layer</name>

    <!-- BLUE: Bazytrack Office -->
    <Style id="bluePoint">
      <IconStyle>
        <color>ffff0000</color> <!-- blue -->
      </IconStyle>
    </Style>

    <Placemark>
      <name>Bazytrack Office</name>
      <styleUrl>#bluePoint</styleUrl>
      <Point>
        <coordinates>35.9106,31.9632,0</coordinates>
      </Point>
    </Placemark>


    <!-- GREEN: Customer A -->
    <Style id="greenPoint">
      <IconStyle>
        <color>ff00ff00</color> <!-- green -->
      </IconStyle>
    </Style>

    <Placemark>
      <name>Customer A</name>
      <styleUrl>#greenPoint</styleUrl>
      <Point>
        <coordinates>35.9231,31.9520,0</coordinates>
      </Point>
    </Placemark>


    <!-- RED: Fuel Station -->
    <Style id="redPoint">
      <IconStyle>
        <color>ff0000ff</color> <!-- red -->
      </IconStyle>
    </Style>

    <Placemark>
      <name>Fuel Station 1</name>
      <styleUrl>#redPoint</styleUrl>
      <Point>
        <coordinates>35.9302,31.9584,0</coordinates>
      </Point>
    </Placemark>

  </Document>
</kml>
